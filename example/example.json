{
    "name": "hello",
    "url": "http://localhost:3000/api/trpc",
    "tests": [
        {
            "name": "valid query",
            "query": {
                "route": "example.hello",
                "input": {
                    "text": "Matt"
                }
            },
            "success": {
                "status": 200
            }
        },
        {
            "name": "bad query",
            "query": {
                "route": "example.hello",
                "input": {
                    "text": "404pls"
                }
            },
            "success": {
                "status": 404
            }
        },
        {
            "name": "data query",
            "query": {
                "route": "example.hello",
                "input": {
                    "text": "Matt"
                }
            },
            "success": {
                "data": {
                    "greeting": "Hello Matt"
                }
            }
        }
    ]
}